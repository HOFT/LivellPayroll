<div class="modal fade" id="JobInfoModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h4 class="modal-title" id="myModalLabel"><i class="fa fa-clock-o "></i> Time Details</h4>
            </div>
            <div class="modal-body">
                <div class="well well-sm">
                    <h2 class="text-primary" style="display: inline;"><strong><span id="TimeCard-JobName">System Desaing</span></strong>   <small> —  <i class="fa fa-pencil"></i>. <cite title="Source Title" id="TimeCard-Date">2017-05-20</cite></small> </h2>
                    <span class="badge bg-color-greenLight pull-right" id="TimeCard-Stu"><i class="fa fa-check"></i>  Paid</span>
                    

                    <div class="bs-example">
                        <input type="hidden" id="TimeCard-Id" value="0">
                        <br />
                        <div class="form-group">
                            <label for="TimeCard-StartTime"><strong><i class="fa fa-history"></i>  Start Time:</strong></label>
                            <div class="input-group">
                                <span class="fc-event-time txt-color-blue font-md" id="TimeCard-StartTime">2017-05-20 12:15:00</span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="TimeCard-StopTime"><strong><i class="fa fa-history"></i> Stop Time:</strong></label>
                            <div class="input-group">
                                <span class="fc-event-time txt-color-blue font-md" id="TimeCard-StopTime">2017-05-20 16:15:00</span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="TimeCard-TotalTime"><strong><i class="fa fa-bell"></i> Total Work Time:</strong></label>
                            <div class="input-group">
                                <span class="fc-event-time txt-color-blue font-md" id="TimeCard-TotalTime">400 (minute)</span>
                                <p class="note"><strong>Note :</strong> The total work time tound to @ViewBag.RoundTo Minutes</p>
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="CustomerJob"><strong><i class="fa fa-bookmark"></i> Time Note:</strong></label>
                            <p id="TimeCard-Note">The Total Work Time Round To  Minutes</p>
                        </div>
                    </div>
                </div>


            </div>
            <div class="modal-footer">
                <button type="button" id="btn-time-delete" class="btn btn-danger">
                    <i class="fa fa-trash-o"></i>
                    Delete
                </button>
                <button type="button" class="btn btn-default" data-dismiss="modal">
                    <i class="fa fa-times"></i>
                    Cancel
                </button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->