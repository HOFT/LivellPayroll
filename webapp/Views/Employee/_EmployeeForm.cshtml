                                            <div class="col-md-8">
                                                <input class="form-control" name="FName" placeholder="Please enter your First Name" type="text">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 control-label">Middle Init</label>
                                            <div class="col-md-8">
                                                <input class="form-control" name="MInit" placeholder="Please enter your Middle Init" type="text">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 control-label">Last Name *</label>
                                            <div class="col-md-8">
                                                <input class="form-control" name="LName" placeholder="Please enter your Last Name" type="text">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 control-label">SSN *</label>
                                            <div class="col-md-8">
                                                <input class="form-control" name="SSN" placeholder="Please enter your SSN" data-inputmask="'mask': '999-99-9999'" type="text">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 control-label">Email *</label>
                                            <div class="col-md-8">
                                                <input class="form-control" name="Email" placeholder="email@address.com" type="text">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 control-label" for="select_time_zone">Time Zone *</label>
                                            <div class="col-md-8">
                                                @Html.DropDownList("TimeZone", ViewBag.TimeZone as SelectList, "Please Select TimeZone", new { @class = "form-control", @id = "select_time_zone" })
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="col-md-4 control-label" for="DefaultJob">Default Job</label>
                                            <div class="col-md-8">
                                                <select class="form-control" id="DefaultJob" name="DefaultJob"></select>
                                            </div>
                                        </div>

                                        @*<div class="form-group checkbox">
                                            <label class="col-md-4 control-label"></label>
                                            <div class="col-md-8"><label><input type="checkbox" name="F124" value="1">Check if inactive</label></div>
                                        </div>*@


                                    </div>
                                    <div class="col-sm-6 col-md-6 col-lg-6">
                                        <div class="form-group">
                                            <label class="col-md-4 control-label">Address1 *</label>
                                            <div class="col-md-8">
                                                <input class="form-control" name="Address1" placeholder="Please enter your Address" type="text">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 control-label">Address2 </label>
                                            <div class="col-md-8">
                                                <input class="form-control" name="Address2" placeholder="Please enter your Address" type="text">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 control-label">City *</label>
                                            <div class="col-md-8">
                                                <input class="form-control" name="City" placeholder="Please enter your City" type="text">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 control-label" for="select_state">State *</label>
                                            <div class="col-md-8">
                                                @Html.DropDownList("State", ViewBag.StatesList as SelectList, "Please Select State", new { @class = "form-control", @id = "select_state" })
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 control-label">Zip *</label>
                                            <div class="col-md-8">
                                                <input class="form-control" id="ZipCode" name="ZipCode" placeholder="Please enter your zipcode" data-inputmask="'mask': '99999'" type="text" />
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-4 control-label">Telephone *</label>
                                            <div class="col-md-8">
                                                <input class="form-control" name="Phone" placeholder="Please enter your PhoneNumber" data-inputmask="'mask': '(999)-999-9999'" type="text">
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="col-md-4 control-label">Role *</label>
                                            <div class="col-md-8">
                                                <label class="radio radio-inline">
                                                    <input type="radio" class="radiobox" name="UserRole" value="Admin">
                                                    <span>Admin</span>
                                                </label>
                                                <label class="radio radio-inline">
                                                    <input type="radio" class="radiobox" name="UserRole" value="Manager">
                                                    <span>Manager</span>
                                                </label>
                                                <label class="radio radio-inline">
                                                    <input type="radio" class="radiobox" name="UserRole" value="Employee">
                                                    <span>Employee</span>
                                                </label>
                                            </div>
                                        </div>

                                        <div class="form-group checkbox">
                                            <label class="col-md-4 control-label"></label>
                                            <div class="col-md-8">
                                                <label>
                                                    <input type="checkbox" name="isPayrollSetup" value="1">
                                                    Setup payroll for this employee
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </fieldset>

                            </div>
                            <div class="step-pane" data-step="2">
                                <h3><i class="fa fa-pencil"> </i> <strong>Paid Time Off (PTO)</strong></h3>
                                <fieldset>
                                    <p class="alert alert-info no-margin">
                                        <button class="close" data-dismiss="alert">
                                            ×
                                        </button>
                                        ● This setup page is optional for states other than california. leave 0 if not needed.<br />
                                        ● The maximum hours are the cap hours an employee may accrue
                                    </p>
