                                        <div class="col-md-10">
                                            <label class="checkbox-inline">
                                                <input type="checkbox" name="printPTOStub" value=1>
                                                <span>Print Paid Benefits on Pay Stub</span>
                                            </label>
                                        </div>

                                    </div>
