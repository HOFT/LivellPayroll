<section id="widget-grid" class="employee_info_section">
    <div class="row">

        <!-- NEW WIDGET START -->
        <article class="col-sm-12 col-md-12 col-lg-12">

            <!-- Widget ID (each widget will need unique ID)-->
            <div class="jarviswidget  jarviswidget-color-darken" id="wid-id-2" data-widget-editbutton="false" data-widget-deletebutton="false">
                <!-- widget options:
                usage: <div class="jarviswidget" id="wid-id-0" data-widget-editbutton="false">

                data-widget-colorbutton="false"
                data-widget-editbutton="false"
                data-widget-togglebutton="false"
                data-widget-deletebutton="false"
                data-widget-fullscreenbutton="false"
                data-widget-custombutton="false"
                data-widget-collapsed="true"
                data-widget-sortable="false"

                -->
                <header>
                    <span class="widget-icon"> <i class="fa fa-align-justify"></i> </span>
                    <h2>Employee Info @Html.Raw(ViewData["employeeInfo"]) </h2>

                </header>

                <!-- widget div-->
                    <!-- widget content -->
                <div class="widget-body fuelux" data-initialize="wizard" id="myWizard">
                    <div class="wizard">
                        <ul class="steps">
                            <li data-step="1" data-name="campaign" class="active">
                                <span class="badge badge-info">1</span> Employee Basic<span class="chevron"></span>
                            </li>
                            <li data-step="2">
                                <span class="badge">2</span>Paid Time Off<span class="chevron"></span>
                            </li>
                            <li data-step="3">
                                <span class="badge">3</span>Payment/Deduction<span class="chevron"></span>
                            </li>
                            <li data-step="4">
                                <span class="badge">4</span>Tax<span class="chevron"></span>
                            </li>
                            <li data-step="5" data-name="template">
                                <span class="badge">5</span>Confirmation<span class="chevron"></span>
                            </li>
                        </ul>
                    </div>
                    <div class="step-content">
                        <form class="form-horizontal" id="fuelux-wizard" method="post" novalidate="novalidate">
                            <div class="step-pane active" data-step="1">
                                <h3><i class="fa fa-pencil"> </i> <strong>User Information</strong></h3>
                                <hr />
