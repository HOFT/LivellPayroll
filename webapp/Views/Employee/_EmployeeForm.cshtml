                                                    <div class="col-md-8">
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="fa fa-dollar"></i></span>
                                                            <input class="form-control" step="0.01" placeholder="0.00" type="number" id="F102" name="F102" disabled="disabled">
                                                        </div>
                                                    </div>
                                                </div>

