                                                    {
                                                        <div class="form-group">
                                                            <label class="col-md-4 control-label">@t.Description</label>
                                                            <div class="col-md-8">
                                                                <div class="input-group">
                                                                    <span class="input-group-addon"><i class="fa fa-dollar"></i></span>
                                                                    <input class="form-control" step="0.01" placeholder="0.00" type="number" name="@t.CodeMap" id="CopyValue_F1002">
