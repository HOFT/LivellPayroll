                    var EndDate = $('#dtpEndDate').val();
                    GetIncomeHous(EmpId, StartDate, EndDate);
                }
            });

            $('#dtpEndDate').datepicker({
                dateFormat: 'yy-mm-dd',
                prevText: '<i class="fa fa-chevron-left"></i>',
                nextText: '<i class="fa fa-chevron-right"></i>',
                onSelect: function (selectedDate) {
                    $('#dtpStartDate').datepicker('option', 'maxDate', selectedDate);

                    var EmpId = $('#cboEmployees').val();
                    var StartDate = $('#dtpStartDate').val();
                    var EndDate = $('#dtpEndDate').val();
                    GetIncomeHous(EmpId, StartDate, EndDate);
                }
            });


            $('#cboEmployees').change(function () {
                var EmpId = $(this).children('option:selected').val();
                var StartDate = $('#dtpStartDate').val();
                var EndDate = $('#dtpEndDate').val();
                $.post("/PayRoll/GetEmpDes", { EmpId: EmpId }, function (data) {
                    for (var key in data.dicVisible) {
                        if (!data.dicVisible[key]) {
                            $('#' + key).parent().hide();
                        };

                    };
                    for (var key in data.dicValue) {//遍历dicValue json对象的每个key/value对,p为key
                        var tag = $('#' + key).get(0).tagName;
                        if (tag == "INPUT") {
                            $('#' + key).val(data.dicValue[key]);
                        } else {
                            $('#' + key).text(data.dicValue[key]);
                        };
                        if (key == "dtpStartDate") {
                            StartDate = data.dicValue[key]
                        };
                        if (key == "dtpEndDate") {
                            EndDate = data.dicValue[key]
                        };
                    };
                    //GetIncomeHous(EmpId, StartDate, EndDate);

                });

            });

            $('#btnCalculate').click(function () {
                var par = CalculatRefurbish();
                if (!par) { return; };
                $.post("/PayRoll/Calculate", par, function (data) {
                    if (data.status == 'success') {
                        for (var key in data.dicValue) {
                            var tag = $('#' + key).get(0).tagName;
                            if (tag == "INPUT") {
                                $('#' + key).val(data.dicValue[key]);
                            } else {
                                $('#' + key).text(data.dicValue[key]);
                            }
                        };
                        
                    };
                });
            });

            $('#btnSaveCheck').click(function () {
                var par = CalculatRefurbish();
                if (!par) { return; };

                if (par.EmpId == null || par.DateStartPeriod == null || par.DateEndPeriod == null) {
                    ErrorBox("Please check whether the employee, time interval is input!");
                    return;
                };
                //var CheckNo = 0;
                //par["CheckNo"] = CheckNo;
                $.ajax({
                    async: false,
                    type: 'POST',
                    url: "/PayRoll/PayrollInputSaveCheck",
                    data: par,
                    dataType: "json",
                    success: function (data) {
                        if (data.code == 2) {
                            ErrorBox(data.message);
                            return;
                        } else if (data.code == 3) {
                            $.SmartMessageBox({
                                title: "Plese Confirmation !",
                                content: data.message,
                                buttons: '[No][Yes]'
                            }, function (ButtonPressed) {
                                if (ButtonPressed === "Yes") {
                                    PayrollInputSave(par);
                                };
                                if (ButtonPressed === "No") {
                                    return;
                                };

                            });
                        } else if (data.code == 1) {
                            PayrollInputSave(par);
                        } else {
                            ErrorBox("Error!");
                        };
                    }

                });
            });

            $('.link-btn').click(function () {
                var TimeType = $(this).attr("data-link-type");
                var EmpId = $('#cboEmployees').val();
                var StartDate = $('#dtpStartDate').val();
                var EndDate = $('#dtpEndDate').val();
                if (typeof (EmpId) == "undefined"|| StartDate == "" || EndDate == "") {
                    ErrorBox("Please check whether the employee, time interval is input!");
                    return;
                };
                $.post("/PayRoll/GetIncomeHous", { EmpId: EmpId, StartDate: StartDate, EndDate: EndDate }, function (data) {
                    $('#' + TimeType).val(data[TimeType]);
                    shake($('#' + TimeType), "BlinkColor", 2);
                    SuccessBox("The total hours is " + data[TimeType]);
                });
            });

        })


    </script>
